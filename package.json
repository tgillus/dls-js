{
  "author": "Tramaine L. Gillus",
  "dependencies": {
    "chevrotain": "11.0.3",
    "date-fns": "4.1.0",
    "effect": "3.10.9",
    "liqe": "3.8.0",
    "ohm-js": "17.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@ohm-js/cli": "2.0.0",
    "@release-it/conventional-changelog": "9.0.2",
    "@types/bun": "1.1.13",
    "@vitest/coverage-v8": "2.1.4",
    "del-cli": "6.0.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "release-it": "17.10.0",
    "testdouble": "3.20.2",
    "typescript": "5.6.3",
    "vitest": "2.1.4"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "name": "dsl",
  "repository": {
    "type": "git",
    "url": "git@github.com:tgillus/dsl.git"
  },
  "scripts": {
    "build": "bun run clean && bun test && tsc",
    "clean": "del ./lib ./coverage",
    "format": "biome format --write --no-errors-on-unmatched .",
    "grammar:ohm": "ohm generateBundles --withTypes --esm 'src/filter/transpiler/parser/grammar/*.ohm'",
    "lint": "biome lint --no-errors-on-unmatched .",
    "lint:fix": "biome lint --apply --no-errors-on-unmatched .",
    "prepare": "husky",
    "release": "release-it --ci",
    "start:chevrotain": "bun src/chevrotain/index.ts",
    "start:ohm": "bun src/ohm/index.ts",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "watch": "tsc -c"
  },
  "type": "module",
  "version": "0.0.0"
}
