{
  "author": "Tramaine L. Gillus",
  "dependencies": {
    "chevrotain": "11.0.3",
    "dayjs": "1.11.11",
    "liqe": "3.8.0",
    "ohm-js": "17.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@ohm-js/cli": "2.0.0",
    "@release-it/conventional-changelog": "8.0.1",
    "@types/node": "20.14.9",
    "@typescript-eslint/eslint-plugin": "7.14.1",
    "@typescript-eslint/parser": "7.14.1",
    "@vitest/coverage-v8": "1.6.0",
    "del-cli": "5.1.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "release-it": "17.4.0",
    "testdouble": "3.20.2",
    "typescript": "5.5.2",
    "vitest": "1.6.0"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "name": "dsl",
  "repository": {
    "type": "git",
    "url": "git@github.com:tgillus/dsl.git"
  },
  "scripts": {
    "build": "bun run clean && bun test && tsc",
    "clean": "del ./lib ./coverage",
    "format": "biome format --write --no-errors-on-unmatched .",
    "grammar:ohm": "ohm generateBundles --withTypes --esm 'src/filter/transpiler/parser/grammar/*.ohm'",
    "lint": "biome lint --no-errors-on-unmatched .",
    "lint:fix": "biome lint --apply --no-errors-on-unmatched .",
    "prepare": "husky",
    "release": "release-it --ci",
    "ohm:start": "bun src/ohm/index.ts",
    "chevrotain:start": "bun src/chevrotain/index.ts",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "watch": "tsc -c"
  },
  "type": "module",
  "version": "0.0.0"
}
